{
  "epochs": 1000,
  "batch_size": 128,
  "window_size": 128,
  "return_sequences": true,
  "scaler": "processing.StandardScaler",
  "model": "models.conv_models.dcc_generic",
  "num_derivatives": 2,
  "deepcast_derivatives": true,
  "model_args": {
    "num_blocks": 7,
    "num_in_conv": 0,
    "num_out_conv": 0,
    "return_sequences": true,
    "merge_blocks": false,

    "b_conv_activation": "relu",
    "b_conv_residual": false,
    "b_conv_padding": "same",
    "b_conv_filters": 16,
    "b_conv_kernel_size": 3,
    "b_conv_bias": false,
    "b_conv_weights_init": "glorot_normal",

    "optimizer": "adam",
    "loss": "mse"
  }
}